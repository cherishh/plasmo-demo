{
  "name": "plasmo-dmo",
  "displayName": "Plasmo dmo",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "cherishh",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@mozilla/readability": "^0.4.4",
    "@plasmohq/messaging": "^0.5.0",
    "plasmo": "0.82.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.241",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "prettier": "3.0.0",
    "typescript": "5.1.6"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "tabs",
      "storage",
      "clipboardWrite"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": []
      }
    ]
  }
}
